{
    "name": "roadiz/abstract-api-theme",
    "description": "Exposes Roadiz content as a public REST API.",
    "type": "library",
    "require": {
        "php":">=7.4",
        "roadiz/roadiz": "~1.7.0 || dev-develop",
        "ramsey/uuid": "^3.8",
        "friendsofsymfony/jsrouting-bundle": "^2.5.3",
        "league/oauth2-server": "^8.2.2",
        "symfony/psr-http-message-bridge": "^2.0",
        "nyholm/psr7": "^1.3",
        "symfony/rate-limiter": "^5.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.3",
        "phpstan/phpstan": "^0.12.54",
        "phpstan/phpstan-doctrine": "^0.12.22",
        "roadiz/dts-generator": "dev-main",
        "roadiz/nodetype-contracts": "dev-main",
        "roadiz/documents": "dev-develop",
        "roadiz/markdown": "dev-develop",
        "roadiz/models": "dev-develop",
        "roadiz/rozier": "dev-develop",
        "roadiz/openid": "dev-develop",
